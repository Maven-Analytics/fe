{
  "plugins": [
    [
      "module-resolver",
      {
        "alias": {
          "#root": "./src"
        }
      },
      "@babel/plugin-transform-runtime"
    ]
  ],
  "env": {
    "development": {
      "presets": [
        "@babel/preset-env",
        "next/babel"
      ]
    },
    "production": {
      "presets": [
        "@babel/preset-env",
        "next/babel"
      ]
    },
    "test": {
      "presets": [
        "@babel/preset-env",
        "@babel/preset-react",
        "next/babel"
      ]
    }
  }
}
